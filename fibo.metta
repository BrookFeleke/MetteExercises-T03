 ; ! (fibo (| 2 Nil) 1 2)
(: fibo (-> Number Number Number Number))
(= (fibo $acc $p $n)
    (if (> (+ $n $p) 100)
        $acc
        (if (== 0 (% (+ $n $p) 2))
            (fibo (+ (+ $n $p) $acc) $n (+ $n $p))
            (fibo $acc $n (+ $n $p))
        )
)
)
! (fibo 2 1 2)