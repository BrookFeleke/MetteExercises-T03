(: Nil (List $t))
(: | (-> $t (List $t) (List $t)))
(: append (-> $t (List $t) (List $t)))
(= (append $t (| $head $tail))
    (if (== $tail Nil)
        (| $head (| $t Nil))
        (| $head (append $t $tail)))
)
